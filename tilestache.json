{
  "cache": {
    "name": "Disk",
    "path": "/tmp/stache",
    "umask": "0000",
    "dirs": "portable",
    "gzip": ["xml", "json"]
	},
  "layers": {
    "power": {
	      "provider":
	      {
		"name": "mapnik", 
		"mapfile": "power.xml"
	      },
	      "metatile": 
	      {
		"rows": 4,
		"columns": 4,
		"buffer": 64
	      }
	},
    "telecoms": {
	      "provider":
	      {
		"name": "mapnik", 
		"mapfile": "telecoms.xml"
	      },
	      "metatile": 
	      {
		"rows": 4,
		"columns": 4,
		"buffer": 64
	      }
	}
  }
}
